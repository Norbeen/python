#check if a number is amalgm

def checkAmalgm(first, second):
    first = first.replace(' ','').lower()
    second = second.replace(' ', '').lower()
    return sorted(first) == sorted(second)

first = "The classroom"
second = "School saster"

if checkAmalgm(first, second):
    print("It is amalgm")
else:
    print("It is not amalgm")
