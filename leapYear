def isLeapYear(year):
  if year%100 ==0:
    if year%400 ==0:
      return True
    else:
      return False
  if year%4 ==0:
      return True
  return False
      
        
  
# Below is code that calls the function so you can see
# its output. Feel free to modify it to help test your code.
print('Is 2012 a leap year?', isLeapYear(2012))
