#check if a number is a palindrome for a given string

def Palindrome(uInput, end):
    for i in range(end):
        if uInput[:] == uInput[::-1]:
            return True
        else:
            return False


output = input("Enter what you want to check is a palindrome in UCASE: ")
end = len(output)-1
if Palindrome(output,end):
    print( str(output) + " is a palindrome")
else:
    print(str(output) + " is not palindrome")
